PhysicsList: QGSP_BERT

#General setting  
Type:      simple
Particle:  mu-

# Energy and positions from elog:339

#Default Direction
#				Theta	Phi
#				deg		deg
Direction:		0        0
ThetaSpread:	5
PhiSpread:		360

# dpf is the pbite factor in the filename. It's the "scaling" we
# use after we find the value in the MC (dp=1.025 MeV/c) that
# best matches the data (3% pbite). And then we just sort of go from
# there.
# MomSpread = 1.025*dpf
MomAmp:		18.16
MomSpread:	0.6151

# Position	x	y	z
#			mm	mm	mm
Position:	1	20	-180
PosSpread:	10	20	0
 
RandMode:       none  #(none/root)
EnergyMode:     gRand #(root/histo/none)
PositionMode:   gRand #(uniform/root/none/gRand/uRand)
DirectionMode:  none  #(none/uniform/histo)
ThetaMode:      gRand #(none/gRand/uRand)
PhiMode:        gRand #(none/gRand/uRand)
TimeMode:       none  #(none/root)
pidMode:        none  #(none/root)
#Default Time
#	         ns
Time:    0